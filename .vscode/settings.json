{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#03b8be",
    "activityBar.background": "#03b8be",
    "activityBar.foreground": "#000000",
    "activityBar.inactiveForeground": "#6e6e6e",
    "activityBarBadge.background": "#ffffff",
    "activityBarBadge.foreground": "#000000",
    "commandCenter.border": "#6e6e6e",
    "sash.hoverBorder": "#03b8be",
    "statusBar.background": "#03b8be",
    "statusBar.foreground": "#000000",
    "statusBarItem.hoverBackground": "#03b8be",
    "statusBarItem.remoteBackground": "#03b8be",
    "statusBarItem.remoteForeground": "#000000",
    "titleBar.activeBackground": "#03b8be",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#18559299",
    "titleBar.inactiveForeground": "#6e6e6e"
  },
  "peacock.color": "#03b8be",
  "cSpell.words": ["informations", "Wagehat"],
  "eslint.useFlatConfig": true,
  "eslint.debug": true,
  "eslint.options": {
    "overrideConfigFile": "eslint.config.mjs"
